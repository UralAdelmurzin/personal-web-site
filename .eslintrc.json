{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended"
		// , 
		// "plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	// "plugins": [
	// 	// "react"
	// ],
	"rules": {
		"react/no-console": "off",
		"react/prop-types": "off",
		"react/no-unescaped-entities": 0,
		"react/react-in-jsx-scope": "off",
		"react/no-unknown-property": "off"
	}
}
